{
  "name": "spiral/nylon-bridge",
  "type": "library",
  "description": "Spiral Framework: Nylon PSR-7/PSR-17 bridge",
  "license": "MIT",
  "authors": [
    {
      "name": "Wolfy-J",
      "email": "wolfy.jd@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "nyholm/psr7": "^1.1",
    "spiral/http": "^2.0",
    "spiral/framework": "^2.0"
  },
  "replace": {
    "zendframework/zend-diactoros": "1.*|2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Nylon\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Nylon\\Tests\\": "tests/"
    }
  }
}
